					</sec:accesscontrollist> --%>
					<sec:accesscontrollist hasPermission="3"
						domainObject="formdialog_demoEmployee_button_import_export"
						permissionDes="导出">
						<td width="120px;"><a href="javascript:void(0);" id="allMenu"
							name="bpm_all_menu" class='easyui-menubutton'
							data-options="menu:'#allmm',iconCls:'icon-all-file'">导出</a>
							<div id="allmm" style="width: 105px;">
								<div id='client_import' name="client_import"
									onclick="demoEmployee.exportClientData()">客户端导出</div>
							</div></td>

						<th>用户选择:</th>
						<input class="easyui-validatebox" type="hidden" name="designerId"
							id="designerId" />
						<td><input class="easyui-validatebox" type="text"
							name="designerName" id="designerName" /></td>
					</sec:accesscontrollist>
				</tr>
			</table>
		</div>
		<table id="dgDemoEmployee"
			data-options="
				fit: true,
				border: false,
				rownumbers: true,
				animate: true,
				collapsible: false,
				fitColumns: true,
				autoRowHeight: false,
				toolbar:'#toolbarExportExcel',
				idField :'id',
				singleSelect: true,
				checkOnSelect: true,
				selectOnCheck: false,
				pagination:true,
				method:'get',
				pageSize:dataOptions.pageSize,
				pageList:dataOptions.pageList,
				striped:true">
			<thead>
				<tr>
					<th data-options="field:'id', halign:'center',checkbox:true"
						width="220">主键</th>

					<th data-options="field:'strength', halign:'center'"
						editor="{type:'validatebox',options:{validType:'maxLength[100]'}}"
						width="220">强度</th>
					<th data-options="field:'strengthid', halign:'center',hidden:true" 
						editor="{type:'validatebox',options:{validType:'maxLength[100]'}}"
						width="220">强度id</th>	

					<th data-options="field:'check',align:'center'"
						editor="{type:'validatebox'}" width="220">校对</th>
						<th data-options="field:'checkid',align:'center',hidden:true"
						editor="{type:'validatebox'}" width="220">校对id</th>

					<th data-options="field:'biaoshen', halign:'center'"
						editor="{type:'validatebox',options:{validType:'maxLength[22]'}}"
						width="220">标审</th>
						<th data-options="field:'biaoshenid', halign:'center',hidden:true"
						editor="{type:'validatebox',options:{validType:'maxLength[22]'}}"
						width="220">标审id</th>

					<th data-options="field:'weight', halign:'center'"
						editor="{type:'validatebox',options:{validType:'maxLength[22]'}}"
						width="220">重量</th>
						<th data-options="field:'weightid', halign:'center',hidden:true"
						editor="{type:'validatebox',options:{validType:'maxLength[22]'}}"
						width="220">重量id</th>

					<th
						data-options="field:'reliablity', halign:'center'"
						editor="{type:'validatebox',options:{validType:'maxLength[22]'}}"
						width="220">可靠性</th>
						<th
						data-options="field:'reliablityid', halign:'center',hidden:true"
						editor="{type:'validatebox',options:{validType:'maxLength[22]'}}"
						width="220">可靠性id</th>

					<th data-options="field:'material', halign:'center'"
						editor="{type:'validatebox',options:{validType:'maxLength[100]'}}"
						width="220">材料(所区)</th>
						<th data-options="field:'materialid', halign:'center',hidden:true"
						editor="{type:'validatebox',options:{validType:'maxLength[100]'}}"
						width="220">材料(所区)id</th>

					<th data-options="field:'standardparts', halign:'center'"
						editor="{type:'validatebox',options:{validType:'maxLength[500]'}}"
						width="220">标准件</th>
						<th data-options="field:'standardpartsid', halign:'center',hidden:true"
						editor="{type:'validatebox',options:{validType:'maxLength[500]'}}"
						width="220">标准件id</th>

					<th data-options="field:'qualityaudit',align:'center'"
						editor="{type:'validatebox',options:{editable:false}}" width="220">质审</th>
						<th data-options="field:'qualityauditid',align:'center',hidden:true"
						editor="{type:'validatebox',options:{editable:false}}" width="220">质审id</th>

				</tr>
			</thead>
		</table>
	</div>
	</div>
	<!--*****************************搜索*********************************  -->
	<div id="searchDialog"
		data-options="iconCls:'icon-search',resizable:true,modal:false,buttons:'#searchBtns'"
		style="width: 904px; height: 340px; height: 0px; padding: 0px; overflow: hidden;">
		<form id="demoEmployee">
			<table class="form_commonTable">
				<tr>
					<th width="10%">强度:</th>
					<td width="40%"><input class="easyui-validatebox" type="text"
						name="employeeCode" /></td>
					<!-- 通用代码
					<th data-options="field:'validFlag',halign:'center',align:'left', formatter: format" editor="{type:'combobox',options:{required:true,panelHeight:'auto',editable:false,valueField:'lookupCode',textField:'lookupName'}}"  width="220">状态</th>
				-->
					<th width="10%">校对:</th>
					<td width="40%"><input class="easyui-validatebox" type="text"
						name="employeeName" /></td>
					<!-- 通用代码
					<th data-options="field:'validFlag',halign:'center',align:'left', formatter: format" editor="{type:'combobox',options:{required:true,panelHeight:'auto',editable:false,valueField:'lookupCode',textField:'lookupName'}}"  width="220">状态</th>
				-->
				</tr>
				<tr>
					<th width="10%">标审:</th>
					<td width="40%"><input title="部门名称" class="inputbox"
						type="hidden" name="dept" id="dept" />
						<div class="">
							<input class="easyui-validatebox" name="deptAlias" id="deptAlias"
								readOnly="readOnly"></input>
						</div></td>
					<!-- 通用代码
					<th data-options="field:'validFlag',halign:'center',align:'left', formatter: format" editor="{type:'combobox',options:{required:true,panelHeight:'auto',editable:false,valueField:'lookupCode',textField:'lookupName'}}"  width="220">状态</th>
				-->
					<th width="10%">性别:</th>
					<td width="40%"><pt6:syslookup name="gender"
							lookupCode="PLATFORM_SEX"
							dataOptions="width:151,editable:false,panelHeight:'auto'">
						</pt6:syslookup></td>
					<!-- 通用代码
					<th data-options="field:'validFlag',halign:'center',align:'left', formatter: format" editor="{type:'combobox',options:{required:true,panelHeight:'auto',editable:false,valueField:'lookupCode',textField:'lookupName'}}"  width="220">状态</th>
				-->
				</tr>
				<tr>
					<th width="10%">年龄:</th>
					<td width="40%"><input class="easyui-validatebox" type="text"
						name="age" /></td>
					<!-- 通用代码
					<th data-options="field:'validFlag',halign:'center',align:'left', formatter: format" editor="{type:'combobox',options:{required:true,panelHeight:'auto',editable:false,valueField:'lookupCode',textField:'lookupName'}}"  width="220">状态</th>
				-->
					<th width="10%">邮箱:</th>
					<td width="40%"><input class="easyui-validatebox" type="text"
						name="email" /></td>
					<!-- 通用代码
					<th data-options="field:'validFlag',halign:'center',align:'left', formatter: format" editor="{type:'combobox',options:{required:true,panelHeight:'auto',editable:false,valueField:'lookupCode',textField:'lookupName'}}"  width="220">状态</th>
				-->
				</tr>
				<tr>
					<th width="10%">家庭住址:</th>
					<td width="40%"><input class="easyui-validatebox" type="text"
						name="address" /></td>
					<!-- 通用代码
					<th data-options="field:'validFlag',halign:'center',align:'left', formatter: format" editor="{type:'combobox',options:{required:true,panelHeight:'auto',editable:false,valueField:'lookupCode',textField:'lookupName'}}"  width="220">状态</th>
				-->
					<th width="10%">电话:</th>
					<td width="40%"><input class="easyui-validatebox" type="text"
						name="phoneNum" /></td>
					<!-- 通用代码
					<th data-options="field:'validFlag',halign:'center',align:'left', formatter: format" editor="{type:'combobox',options:{required:true,panelHeight:'auto',editable:false,valueField:'lookupCode',textField:'lookupName'}}"  width="220">状态</th>
				-->
				</tr>
				<tr>
					<th width="10%">状态:</th>
					<td width="40%"><pt6:syslookup name="status"
							lookupCode="PLATFORM_VALID_FLAG"
							dataOptions="width:151,editable:false,panelHeight:'auto'">
						</pt6:syslookup></td>
					<!-- 通用代码
					<th data-options="field:'validFlag',halign:'center',align:'left', formatter: format" editor="{type:'combobox',options:{required:true,panelHeight:'auto',editable:false,valueField:'lookupCode',textField:'lookupName'}}"  width="220">状态</th>
				-->
				</tr>
			</table>
		</form>
		<div id="searchBtns" class="datagrid-toolbar foot-formopera">
			<table class="tableForm" border="0" cellspacing="1" width='100%'>
				<tr>
					<td align="right"><a class="easyui-linkbutton primary-btn"
						iconCls="" plain="false" onclick="demoEmployee.searchData();"
						href="javascript:void(0);">查询</a> <a class="easyui-linkbutton"
						iconCls="" plain="false" onclick="demoEmployee.clearData();"
						href="javascript:void(0);">清空</a> <a class="easyui-linkbutton"
						iconCls="" plain="false" onclick="demoEmployee.hideSearchForm();"
						href="javascript:void(0);">返回</a></td>
				</tr>
			</table>
		</div>
	</div>
	<script
		src="avicit/discussion_manage/inforconfiguration/js/DemoEmployee.js"
		type="text/javascript"></script>
	<script type="text/javascript">
		
		$.extend($.fn.validatebox.defaults.rules, {
			maxLength : {
				validator : function(value, param) {
					return param[0] >= value.length;
				},
				message : '请输入最多 {0} 字符.'
			}
		});
		var demoEmployee;
		$(function() {
			demoEmployee = new DemoEmployee('dgDemoEmployee', 'searchDialog',
					'${url}', 'demoEmployee');
			//var deptDeptCommonSelector = new CommonSelector("dept","deptSelectCommonDialog", "dept", "deptAlias");
			//deptDeptCommonSelector.init();

			var userCommonSelector = new CommonSelector("user",
					"userSelectCommonDialog", "designerId", "designerName",
					null, null, null, false);
			userCommonSelector.init();

			var array = [];

			var searchObject = {
				name : '编号',
				field : 'EMPLOYEE_CODE',
				type : 1,
				dataType : 'string'
			};
			array.push(searchObject);

			var searchObject = {
				name : '名称',
				field : 'EMPLOYEE_NAME',
				type : 1,
				dataType : 'string'
			};
			array.push(searchObject);

			var searchObject = {
				name : '所属部门',
				field : 'DEPT',
				type : 1,
				dataType : 'dept'
			};
			array.push(searchObject);

			var searchObject = {
				name : '性别',
				field : 'GENDER',
				type : 1,
				dictCode : 'PLATFORM_SEX',
				dataType : 'dict'
			};
			array.push(searchObject);

			var searchObject = {
				name : '年龄',
				field : 'AGE',
				type : 1,
				dataType : 'number'
			};
			array.push(searchObject);

			var searchObject = {
				name : '邮箱',
				field : 'EMAIL',
				type : 1,
				dataType : 'string'
			};
			array.push(searchObject);

			var searchObject = {
				name : '家庭住址',
				field : 'ADDRESS',
				type : 1,
				dataType : 'string'
			};
			array.push(searchObject);

			var searchObject = {
				name : '电话',
				field : 'PHONE_NUM',
				type : 1,
				dataType : 'number'
			};
			array.push(searchObject);

			var searchObject = {
				name : '状态',
				field : 'STATUS',
				type : 1,
				dictCode : 'PLATFORM_VALID_FLAG',
				dataType : 'dict'
			};
			array.push(searchObject);
			///

			selfDefQury.init(array);
			selfDefQury.setQuery(function(param) {
				demoEmployee.searchDataBySfn(param);
			});

		});
		function formateDate(value, row, index) {
			return demoEmployee.formate(value);
		};
		function formateDateTime(value, row, index) {
			return demoEmployee.formateTime(value);
		};
		function formategender(value) {
			return Platform6.fn.lookup.formatLookupType(value,
					demoEmployee.gender);
		}
		function formatestatus(value) {
			return Platform6.fn.lookup.formatLookupType(value,
					demoEmployee.status);
		}
		/**通用代码格式化
		function formatvalidFlag(value){
		return Platform6.fn.lookup.formatLookupType(value,demoEmployee.validFlag);
		};
		 **/
	</script>
</body>
</html>