		t1.attribute_02 ,
		t1.attribute_03 ,
		t1.attribute_04 ,
		t1.attribute_05 ,
		t1.attribute_06 ,
		t1.attribute_07 ,
		t1.attribute_08 ,
		t1.attribute_09 ,
		t1.attribute_10
		from process_assignment t1
		where t1.str_id = #{sid}
	</select>


	<!-- 初始化vci树 -->
	<select id="gettreeProcessassignmentByStrId" parameterType="java.lang.String"
		resultMap="StructuralRelationshipMap">
		SELECT SR.ID,
		NVL(SM.CLASS_NAME, ' ') AS CLASS_NAME,
		SR.STR_ID,
		SR.PARENT_STR_ID,
		(SELECT ID
		FROM STRUCTURAL_RELATIONSHIP SSR
		WHERE SSR.STR_ID = SR.PARENT_STR_ID
		AND ROWNUM = 1) AS PARENT_ID,
		NVL(SM.CLASS_CODE, ' ') AS CLASS_CODE,
		NVL(SM.STATUS, ' ') AS STATUS,
		NVL(SM.EDITION, ' ') AS EDITION
		FROM STRUCTURAL_RELATIONSHIP SR
		LEFT
		JOIN STRUCTURE_MANAGE SM
		ON SR.STR_ID = SM.ID
		CONNECT BY PRIOR SR.STR_ID
		= SR.PARENT_STR_ID
		START WITH SR.STR_ID= #{strId,jdbcType=VARCHAR}
	</select>

	<!-- 初始化零件对应的路线 -->
	<select id="getprocessRoteByStrId" parameterType="java.lang.String"
		resultMap="ProcessRouteMap">
		SELECT
		T.ID,
		T.THERMAL_UNIT,
		T.MANUFACTURING_UNIT,
		T.USE_UNIT
		FROM PROCESS_ROUTE T
		WHERE T.STR_ID 
		= #{strId,jdbcType=VARCHAR}
	</select>


</mapper>