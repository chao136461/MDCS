	<!-- 主键查询厂区对象 DISCUSSION_MANAGE -->
	<select id="findFactoryDeptUserByStrId" resultMap="deptUsersDTOMap">
		SELECT TEMP.WORKSHOP_ID, TEMP.DUTY_USER_ID
		FROM (SELECT T.STR_ID,
		T.WORKSHOP_ID,
		WM_CONCAT(F.DUTY_USER_ID) AS DUTY_USER_ID
		FROM FACTORY_COMPLEX T
		LEFT JOIN FACTORY_CENSOR_MANAGE F
		ON T.ID = F.FC_ID
		GROUP BY T.WORKSHOP_ID, T.STR_ID) TEMP
		where 1=1
		<if test="id != null and id != ''">
			and TEMP.STR_ID =#{id}
		</if>
		<if test="array!= null and array != ''">
			and TEMP.WORKSHOP_ID IN
			<foreach collection="array" item="item" index="index" 
			 open="(" close=")" separator=",">  
			#{item}
		</foreach>
		</if>
		ORDER BY TEMP.DUTY_USER_ID ASC
	</select>
	<!-- 主键查询所区对象 DISCUSSION_MANAGE -->
	<select id="findMajorDeptUserByStrId" 
		resultMap="deptUsersDTOMap">
		SELECT TEMP.WORKSHOP_ID, TEMP.DUTY_USER_ID
		FROM (SELECT T.STR_ID,
		T.STD_ID AS WORKSHOP_ID,
		WM_CONCAT(F.DUTY_USER_ID) AS DUTY_USER_ID
		FROM MAJOR_MANAGE T
		LEFT JOIN INSTITUTE_CENSOR_MANAGE F
		ON T.ID = F.MAJ_ID
		GROUP BY T.STD_ID, T.STR_ID) TEMP
		where 1=1
		<if test="id != null and id != ''">
			and TEMP.STR_ID =#{id}
		</if>
		<if test="array!= null and array != ''">
			and TEMP.WORKSHOP_ID IN
			<foreach collection="array" item="item" index="index" 
			 open="(" close=")" separator=",">  
			#{item}
		</foreach>
		</if>
		ORDER BY TEMP.DUTY_USER_ID ASC
	</select>
	<!-- 主键查询对象 DISCUSSION_MANAGE -->
	<select id="findDeptUserByStrId" resultMap="deptUsersDTOMap">
		SELECT TEMP.WORKSHOP_ID, TEMP.DUTY_USER_ID
		FROM (SELECT T.STR_ID,
		T.WORKSHOP_ID,
		WM_CONCAT(F.DUTY_USER_ID) AS DUTY_USER_ID
		FROM FACTORY_COMPLEX T
		LEFT JOIN FACTORY_CENSOR_MANAGE F
		ON T.ID = F.FC_ID
		GROUP BY T.WORKSHOP_ID, T.STR_ID
		UNION ALL
		SELECT T.STR_ID,
		T.STD_ID AS WORKSHOP_ID,
		WM_CONCAT(F.DUTY_USER_ID) AS DUTY_USER_ID
		FROM MAJOR_MANAGE T
		LEFT JOIN INSTITUTE_CENSOR_MANAGE F
		ON T.ID = F.MAJ_ID
		GROUP BY T.STD_ID, T.STR_ID) TEMP
		where 1=1
		<if test="id != null and id != ''">
			and TEMP.STR_ID =#{id}
		</if>
		<if test="array!= null and array != ''">
			and TEMP.WORKSHOP_ID IN
			<foreach collection="array" item="item" index="index" 
			 open="(" close=")" separator=",">  
			#{item}
		</foreach>
		</if>
		ORDER BY TEMP.DUTY_USER_ID ASC
	</select>


